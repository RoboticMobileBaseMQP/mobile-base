<launch>
  <arg name="arm" default="panda" /> <!-- determines which arm to launch-->

  <group if="$(eval arm == 'panda')">
    <include file="$(find mobile_base_simulation)/launch/panda_and_base.launch" />
  </group>

  <group if="$(eval arm == 'kortex')">
    <include file="$(find mobile_base_simulation)/launch/kortex_and_base.launch" />
  </group>
</launch>